
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Upload Excel</h2>
@using(Html.BeginForm("Index", "UploadExcel", FormMethod.Post, new {enctype = "multipart/form-data"}))
            {
                <div>
                    @Html.TextBox("file", "", new {type="file"})<br />
                    <input type="submit" value="Upload"/>
                </div>
            }

